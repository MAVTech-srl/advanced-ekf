{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ROS: Attach",
            "type": "ros",
            "request": "attach"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/install/slam_tools/lib/slam_tools/pointcloud2pcd",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },

        {
            "name": "ROS: Launch",
            "type": "ros",
            "request": "launch",
            "target": "/workspaces/fast_lio_ros2/src/slam_tools/launch/slam_avia.launch.py",
            "preLaunchTask": "colcon_debug"
        },
        {
            "name": "Launch single ROS2 node",
            "request": "launch",
            "type": "cppdbg",
            "miDebuggerServerAddress": "localhost:3000",
            "cwd": "/home/rosdev/",
            // "program": "${workspaceFolder}/install/slam_tools/lib/slam_tools/pointcloud2pcd",
            "program": "${workspaceFolder}/install/advanced-ekf/lib/advanced-ekf/advanced_odometry",
            // "preLaunchTask": "ROS2 node pre-debug advanced_odometry",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "postRemoteConnectCommands": [
                {
                    "text": "source /etc/gdb/gdbinit"
                }
            ],
        }

    ]
}